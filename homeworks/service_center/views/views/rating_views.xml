<odoo>
  <record id="view_service_rating_list" model="ir.ui.view">
    <field name="name">service.rating.list</field>
    <field name="model">service.rating</field>
    <field name="arch" type="xml">
      <list>
        <field name="order_id"/>
        <field name="customer_id"/>
        <field name="technician_id"/>
        <field name="score"/>
        <field name="rating_date"/>
        <field name="comment"/>
      </list>
    </field>
  </record>

  <record id="view_service_rating_form" model="ir.ui.view">
    <field name="name">service.rating.form</field>
    <field name="model">service.rating</field>
    <field name="arch" type="xml">
      <form string="Baholash">
        <sheet>
          <group>
            <field name="order_id"/>
            <field name="customer_id"/>
            <field name="technician_id"/>
            <field name="score"/>
            <field name="rating_date"/>
            <field name="comment"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="action_service_rating" model="ir.actions.act_window">
    <field name="name">Baholar</field>
    <field name="res_model">service.rating</field>
    <field name="view_mode">list,form</field>
  </record>
</odoo>
